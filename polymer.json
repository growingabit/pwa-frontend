{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/signup-step0.html",
    "src/signup-step1.html",
    "src/signup-step2.html",
    "src/not-found.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
